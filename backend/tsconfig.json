{
	"compilerOptions": {
		"lib": ["es2020", "dom"],
		"moduleResolution": "node",
		"sourceMap": true,
		"strict": true,
		"target": "es2019",
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true,
		"module": "commonjs",
		"preserveConstEnums": true,
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"baseUrl": "."
	},
	"exclude": ["node_modules"]
}
